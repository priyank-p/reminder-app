{
  "name": "reminder-app",
  "version": "1.0.0",
  "description": "Reminder app in node.js",
  "main": "index.js",
  "dependencies": {
    "@fortawesome/fontawesome-free-webfonts": "1.0.9",
    "body-parser": "1.18.3",
    "compression": "1.7.2",
    "dateformat": "3.0.3",
    "escape-html": "1.0.3",
    "express": "4.16.3",
    "express-handlebars": "3.0.0",
    "express-minify-html": "0.12.0",
    "fs-extra": "6.0.1",
    "helmet": "3.12.1",
    "level-uplevel": "2.0.0"
  },
  "devDependencies": {
    "clean-webpack-plugin": "0.1.19",
    "css-hot-loader": "1.3.9",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "file-loader": "1.1.11",
    "glob": "7.1.2",
    "http-proxy-middleware": "0.18.0",
    "mini-css-extract-plugin": "0.4.0",
    "node-sass": "4.9.0",
    "nodemon": "1.17.5",
    "nyc": "12.0.2",
    "sass-loader": "7.0.3",
    "source-map-loader": "0.2.3",
    "webpack": "4.11.1",
    "webpack-bundle-tracker": "0.3.0",
    "webpack-cli": "3.0.3",
    "webpack-dev-server": "3.1.4"
  },
  "scripts": {
    "dev": "./tools/dev-server",
    "build": "webpack --config tools/webpack.config.js -p --env production",
    "coverage": "nyc ./tools/test-all",
    "prod": "npm run build && node tools/run-migrations && node app",
    "test": "./tools/test-all",
    "migrations": "./tools/run-migrations -d",
    "lint": "eslint . --cache --cache-location=var/.eslintcache"
  },
  "nyc": {
    "report-dir": "var/nyc",
    "temp-directory": "var/.nyc_output",
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "keywords": [],
  "author": "Priyank Patel <priyankp390@gmail.com>",
  "license": "MIT"
}
