{
  "name": "reminder-app",
  "version": "1.0.0",
  "description": "Reminder app in node.js",
  "main": "index.js",
  "dependencies": {
    "express": "4.16.3",
    "express-handlebars": "3.0.0",
    "express-minify-html": "0.12.0",
    "source-map-loader": "0.2.3",
    "webpack": "4.6.0",
    "webpack-bundle-tracker": "0.3.0",
    "webpack-cli": "2.0.15"
  },
  "devDependencies": {
    "http-proxy-middleware": "0.18.0",
    "nodemon": "1.17.3",
    "nyc": "11.7.1",
    "webpack-dev-server": "3.1.3"
  },
  "scripts": {
    "start": "nodemon tools/dev-server.js --config tools/nodemon.json",
    "build": "webpack --config tools/webpack.config.js -p --env production",
    "coverage": "nyc ./tools/run-tests"
  },
  "nyc": {
    "report-dir": "var/nyc",
    "temp-directory": "var/.nyc_output",
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "keywords": [],
  "author": "Priyank Patel <priyankp390@gmail.com>",
  "license": "MIT"
}
