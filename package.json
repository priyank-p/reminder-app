{
  "name": "reminder-app",
  "version": "1.0.0",
  "description": "Reminder app in node.js",
  "main": "index.js",
  "dependencies": {
    "body-parser": "1.18.3",
    "compression": "1.7.2",
    "express": "4.16.3",
    "express-handlebars": "3.0.0",
    "express-minify-html": "0.12.0",
    "helmet": "3.12.0"
  },
  "devDependencies": {
    "clean-webpack-plugin": "0.1.19",
    "css-hot-loader": "1.3.9",
    "css-loader": "0.28.11",
    "file-loader": "1.1.11",
    "http-proxy-middleware": "0.18.0",
    "mini-css-extract-plugin": "0.4.0",
    "node-sass": "4.9.0",
    "nodemon": "1.17.4",
    "nyc": "11.8.0",
    "sass-loader": "7.0.1",
    "source-map-loader": "0.2.3",
    "webpack": "4.8.1",
    "webpack-bundle-tracker": "0.3.0",
    "webpack-cli": "2.1.3",
    "webpack-dev-server": "3.1.4"
  },
  "scripts": {
    "start": "nodemon tools/dev-server.js --config tools/nodemon.json",
    "build": "webpack --config tools/webpack.config.js -p --env production",
    "coverage": "nyc ./tools/run-tests",
    "prod": "npm run build && node app"
  },
  "nyc": {
    "report-dir": "var/nyc",
    "temp-directory": "var/.nyc_output",
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "keywords": [],
  "author": "Priyank Patel <priyankp390@gmail.com>",
  "license": "MIT"
}
