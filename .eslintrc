{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": 0,
    "indent": [ "error", 2 ],
    "quotes": [ "error", "single" ],
    "semi": [ "error", "always" ],
    "no-trailing-spaces": "error"
  },
  "globals": {
    "publicKey": false,
    "swPath": false
  }
}
